  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "mocha --require ts-node/register tests/**/test.*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SunderShadow/JS-extendable-calculator.git"
  },
  "bugs": {
    "url": "https://github.com/SunderShadow/JS-extendable-calculator/issues"
  },
  "devDependencies": {
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "@types/mocha": "^7.0.2",
    "@types/node": "^18.11.9",
    "mocha": "^7.2.0",
    "ts-node": "^9.1.1"
  }
}
